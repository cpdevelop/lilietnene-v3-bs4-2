{
  "name": "bs4-handlebers-starter",
  "version": "1.0.0",
  "main": "index.js",
  "private": "true",
  "nodemonConfig": {
    "ignore": [
      "git/*",
      "build/*",
      "gulp/*",
      "dist/*",
      "node_modules/*",
      ".eslintrc.js",
      ".gitignore",
      "gulpfile.js",
      "note",
      "packege-lock.json",
      "packege.json"
    ],
    "delay": "2500"
  },
  "scripts": {
    "gulp": "gulp",
    "pregulp": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/styles/app.scss src/styles/app.css",
    "compile": "gulp compileHtml:watch",
    "svg": "gulp icon",
    "clean": "rimraf dist",
    "postclean": "mkdir dist",
    "webpack": "concurrent \"rimraf dist/*js\" webpack",
    "sass": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/styles/app.scss dist/styles/app.css",
    "postsass": "postcss --config build/postcss.config.js --replace \"dist/styles/*.css\" \"!dist/styles/*.min.css\"",
    "watch:server": "browser-sync start --server dist",
    "watch:js": "nodemon --ignore templates/ --ignore data/ --ignore styles/ -e js -x \"npm run webpack\"",
    "watch:sass": "nodemon --ignore templates/ --ignore data/ --ignore js/ -e sass -x \"npm run sass\"",
    "watch": "concurrent \"npm run watch:js\" \"npm run watch:sass\" \"npm run watch:server\""
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.23.5",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.8",
    "del": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-data": "^1.3.1",
    "gulp-handlebars-html": "0.0.2",
    "gulp-regex-rename": "^0.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-svg-sprite": "^1.3.7",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.11",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.10",
    "require-dir": "^0.3.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.3",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9"
  }
}
